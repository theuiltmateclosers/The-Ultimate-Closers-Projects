{
  "name": "The Ultimate Closers",
  "projectId": "764836c7-4acb-4f95-864f-ce0a767d6c78",
  "framework": "next",
  "buildCommand": "cd apps/web && npm install && npm run build",
  "outputDirectory": "apps/web/.next",
  "installCommand": "npm install",
  "devCommand": "cd apps/web && npm run dev",
  "rootDirectory": "./",
  "envVars": {
    "NEXT_PUBLIC_API_URL": "https://api.theultimateclosers.com",
    "NODE_ENV": "production"
  },
  "deployment": {
    "autoDeploy": true,
    "branch": "main",
    "preview": true
  },
  "features": {
    "auth": true,
    "admin": true,
    "triggers": true,
    "animations": true
  }
}
